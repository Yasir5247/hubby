# Apple account
FASTLANE_USER=me@joshholtz.com
FASTLANE_TEAM_NAME=Josh Holtz
FASTLANE_ITC_TEAM_NAME=Josh Holtz

PRODUCE_APP_IDENTIFIER=com.joshholtz.hubby
PRODUCE_APP_NAME=Hubby - Your GitHub Bestie
PRODUCE_VERSION=0.1.0
PRODUCE_SKU=HUBBY_001
PRODUCE_PLATFORMS=ios,osx

# Match
MATCH_USERNAME=me@joshholtz.com
MATCH_GIT_URL=https://github.com/joshdholtz/fastlane-match-repo.git
MATCH_APP_IDENTIFIER=com.joshholtz.hubby
